#include <iostream>



using namespace std;



int Min(int A[],int length){

    int min = A[0];

    for(int i=1; i<length; i++){

        if(A[i]<min){

            min=A[i];

        }

    }

    return min;

}



int main(){

    int numbers[]={3,6,7,2,4,5};

    cout << Min(numbers,sizeof (numbers)/sizeof(numbers [0]));

    return 0;

}
