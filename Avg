#include <iostream>



using namespace std;





double Average(double A[],int count){

    double sum=0;

    double avg;

    for(int i=0; i<=count; i++){

        sum=sum+A[i];

        avg=sum/count;

    }

    return avg;

}

 int main(){

    double numbers[]={3.0,5.0,1.0,6.0,5.0,4.0};

    cout << Average(numbers,sizeof (numbers ) /sizeof(numbers [0]));

    return 0;
